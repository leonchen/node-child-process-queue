  },
  "homepage": "https://github.com/leonchen/node-child-process-queue#readme",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "testEnvironment": "node",
